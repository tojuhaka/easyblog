<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      metal:use-macro="base">
  <tal:block metal:fill-slot="content">
    <div class="row">
      <div class="page-header span4">
      </div>
      <span tal:condition="provider" tal:replace="structure provider('editbar')" />
    </div><!-- row -->
    <div class="row">
    <div class="list-items">
        <ul class="content">
          <span tal:omit-tag="" tal:repeat="blog context.order_by_time()">
           <li>
             
             <div tal:omit-tag="python: repeat.blog.number % 3 == 0" class="row">

               <div class="span4">
                <p> lol </p>
               </div>
             </div>
            <!-- <a tal:attributes="href resource_url(post, request)" tal:content="post.title"></a> -->
          </li> 
          </span>
        </ul>	
        <div tal:condition="python: repeat.post.length > 15" class="page_navigation"></div>
	</div>		
    </div> <!-- ROW -->
  </tal:block>
</html>
