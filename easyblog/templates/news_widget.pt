      xmlns:i18n="http://xml.zope.org/namespaces/i18n" >
        <h2 i18n:translate="latest_news">Latest news </h2>
        <div tal:repeat="news_item news" class="accordion-group">
          <div class="accordion-heading">
            <a tal:content="news_item['title']" class="accordion-toggle" href="#collapseOne" data-parent="#accordion2" data-toggle="collapse"> Collapsible Group Item #1 </a>
          </div>
          <div id="collapseOne" class="accordion-body collapse" style="height: 0px;">
            <div tal:content="news_item['text']" class="accordion-inner"> Some text. </div>
          </div>
        </div><!-- accordion-group -->
